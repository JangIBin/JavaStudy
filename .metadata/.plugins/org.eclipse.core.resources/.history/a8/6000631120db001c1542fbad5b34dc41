package inher;

class Book {
	
	static int count;
	
	String title;
	int score;
	
	
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public int getScore() {
		return score;
	}
	public void setScore(int score) {
		this.score = score;
	}
	
	@Override
	private String toString() {

		return "Book[title = " + title + ", score = " + score + "]";
		
	}
	
}

