package classexam;

import java.io.*;

public class ClassExam01 {

	public static void main(String[] args) throws IOException{
		
		ClassExam ce = new ClassExam();
		// 값 입력
		ce.setX();
		ce.setY();
		
		// 결과 출력
		ce.display();
		
		// 좌표 이동
		ce.getX(50);
		ce.getY(80);

		// 결과 출력
		ce.display();
				
	}

}
