package com.constex;

public class Circle { // 클래스
	
	int radius; // 속성, 멤버변수, 필드
	String name;
	
	public Circle() { // 생성자 메소드 : 생성자의 이름은 클래스의 이름과 같다.
		// 필드 초기화
		radius = 1;
		name = "";
		

	}
	public 패ㅑㅜㅇ Circle(int r, String n) { // 매개변수를 활용하여 필드 초기화
		// 매개변수를 가진 생성자
		// 생성자에 return타입은 없음
		radius = r;
		name = n;
		
	}

}
