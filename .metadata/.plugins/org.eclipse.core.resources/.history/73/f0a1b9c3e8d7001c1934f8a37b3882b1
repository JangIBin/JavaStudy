package com.constex;

class ThisEx {
	
	String name, jumin, tel;
	
	public ThisEx() {
		this.name = "GamJa";
		this.jumin = "012345-6789012";
		tel = "010-1234-5678";
	}
	
	public ThisEx(String name) {
		this();
		this.name = name;
		
	}

	public ThisEx(String name, String jumin) {
		this(name);
		this.jumin = jumin;
		
	}
	
}

public class ThisEx04 {

	public static void main(String[] args) {
		
	}
	
}
